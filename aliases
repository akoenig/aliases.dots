# Jump to project directory
alias p="cd $HOME/projects"

# Shorter "git" :)
alias g="git"
alias gpom="git push origin master"
alias gpod="git push origin develop"
alias gp="git push"
alias gpoc="git rev-parse --abbrev-ref HEAD | xargs git push origin"
alias gclog="git log --oneline --decorate"

# Whats my ip address, mate?
alias ip="curl ip.appspot.com"

# Shorter vim
alias v="vim"

# better ll
alias ll="ls -lah"

